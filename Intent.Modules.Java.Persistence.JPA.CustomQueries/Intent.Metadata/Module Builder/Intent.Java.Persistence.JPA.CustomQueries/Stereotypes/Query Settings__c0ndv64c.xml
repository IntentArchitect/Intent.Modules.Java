      <isActiveFunction>function hasColumnIncludedInResult(element) {
    if (element.specialization === "Column" &amp;&amp;
        element.getStereotype("Column Settings")?.getProperty("Include in result")?.value) {
        return true;
    }

    return element.getChildren().some(hasColumnIncludedInResult);
}

return element.getChildren().some(hasColumnIncludedInResult);</isActiveFunction>
